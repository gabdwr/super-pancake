# ========================================
# EXAMPLE ENVIRONMENT CONFIGURATION
# ========================================
# Copy this file to .env and fill in your actual values
# NEVER commit .env to git - it's already in .gitignore

# ========================================
# API KEYS
# ========================================

# Moralis API (blockchain data)
MORALIS_API_KEY=your_moralis_api_key_here

# Alchemy BSC RPC (blockchain connection)
ALCHEMY_BSC_RPC=https://bnb-mainnet.g.alchemy.com/v2/YOUR_KEY_HERE

# CoinGecko API (optional, for price data)
COINGECKO_API_KEY=your_coingecko_api_key_here

# BSCScan API (blockchain explorer)
BSCSCAN_API_KEY=your_bscscan_api_key_here

# ========================================
# TELEGRAM ALERTS
# ========================================

TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# ========================================
# SUPABASE DATABASE
# ========================================

SUPABASE_HOST=db.xxxxx.supabase.co
SUPABASE_PORT=5432
SUPABASE_USERNAME=postgres
SUPABASE_PASSWORD=your_supabase_password_here
SUPABASE_DBNAME=postgres

SUPABASE_URL=https://xxxxx.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ========================================
# PAPER TRADING SETTINGS
# ========================================

PAPER_TRADING=True
PAPER_TRADING_BALANCE=1000
MAX_POSITION_SIZE=50
MAX_OPEN_POSITIONS=10

# ========================================
# RISK MANAGEMENT
# ========================================

STOP_LOSS_PERCENT=-50
TAKE_PROFIT_T1_PERCENT=100
TAKE_PROFIT_T2_PERCENT=300

# ========================================
# TOKEN FILTER CRITERIA (Legacy - not used by critical filters)
# ========================================

MIN_TOKEN_AGE_DAYS=7
MAX_TOKEN_AGE_DAYS=30
MIN_MARKET_CAP_USD=500000
MAX_MARKET_CAP_USD=5000000
MIN_LIQUIDITY_USD=50000
MIN_GOPLUS_SCORE=70
MAX_GINI_COEFFICIENT=0.7

# ========================================
# DISCOVERY FILTER SETTINGS
# ========================================

DISCOVERY_MIN_LIQUIDITY=5000
DISCOVERY_MAX_AGE_DAYS=200
DISCOVERY_MIN_SCORE=60
DISCOVERY_ALERT_THRESHOLD=55
DISCOVERY_USE_PROFILES=True

# ========================================
# CRITICAL FILTERS - YOUR SECRET STRATEGY
# ========================================
# These are the actual thresholds used to filter tokens
# Adjust these to tune your edge - these values are NOT committed to git
#
# IMPORTANT: These are your competitive advantage!
# The defaults below are generic starting points.
# Tune these based on backtesting and analysis.

# Filter 1: Honeypot Detection
FILTER_ALLOW_HONEYPOT=False

# Filter 2: LP Lock Percentage (min % of LP tokens locked)
# Lower = more risky but catches earlier opportunities
# Higher = safer but might miss early movers
FILTER_MIN_LP_LOCKED=30

# Filter 3: Concentration Score (liquidity concentration, 0-100)
# Lower = more fragmented liquidity (risky)
# Higher = more concentrated (safer)
FILTER_MIN_CONCENTRATION=50

# Filter 4: Minimum Liquidity USD
# Lower = catch tokens earlier but more risky
# Higher = safer but miss early opportunities
FILTER_MIN_LIQUIDITY_USD=20000

# Filter 5: Maximum Buy Tax (%)
# Most legitimate tokens have <5% buy tax
# Scams often have 10%+ taxes
FILTER_MAX_BUY_TAX=10

# Filter 6: Maximum Sell Tax (%)
# Watch out for high sell taxes (honeypot indicator)
# Most legitimate tokens have <5% sell tax
FILTER_MAX_SELL_TAX=10

# Filter 7: Mintable Token Detection
# Mintable = dev can create new tokens (inflate supply)
FILTER_ALLOW_MINTABLE=False

# ========================================
# GRADUATION SYSTEM
# ========================================

GRADUATION_CONSECUTIVE_PASSES=5
GRADUATION_HOURLY_CHECKS=True
GRADUATION_DAILY_CHECKS=True

# ========================================
# LOGGING
# ========================================

LOG_LEVEL=INFO
LOG_TO_FILE=True
LOG_TO_CONSOLE=True

# ========================================
# DEVELOPMENT MODE
# ========================================

DEBUG_MODE=True
